<view><view class="tab" style="{{('top:'+tabTopPX+'PX')}}"><view data-event-opts="{{[['tap',[['cutTab',[0]]]]]}}" class="{{['tab_item',(activeIndex==0)?'active':'']}}" bindtap="__e">待签约</view><view data-event-opts="{{[['tap',[['cutTab',[1]]]]]}}" class="{{['tab_item',(activeIndex==1)?'active':'']}}" bindtap="__e">已签约</view></view><view class="list"><block wx:if="{{list.length>0}}"><block><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="sign_item"><view class="title">{{item.companyName}}</view><view class="container"><view class="content"><view style="margin-bottom:5rpx;">{{"《"+item.desc+"》"}}</view><view>{{item.date}}</view></view><block wx:if="{{activeIndex==0}}"><view data-event-opts="{{[['tap',[['goPDF',['$0'],[[['list','',index]]]]]]]}}" class="signbtn" bindtap="__e">去签约</view></block><block wx:if="{{activeIndex==1}}"><view data-event-opts="{{[['tap',[['goPDF',['$0'],[[['list','',index]]]]]]]}}" class="signbtn" bindtap="__e">查看</view></block></view></view></block></block></block><block wx:else><view>暂无数据</view></block></view></view>